<!DOCTYPE HTML>
<html lang="pt-br">
	<head>
		<meta charset="UTF-8">

    <!-- JQuery -->
		<script src="js/jquery-2.2.1.js"></script>

		<!-- bootstrap - link cdn -->
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
	
		<!-- incluindo css -->
		<link href="css/app.css" rel="stylesheet">

		<!-- favicon -->
        <link rel="shortcut icon" href="images/favicon.ico" />
	</head>
	<body>
	<div class="span6">
		<alert ng-repeat="alert in alerts" type="alert.type"
			close="closeAlert($index)"></alert>
	
		<h3></h3>
		 <hr/>
		 <div class="form-group">
			<div class="col-lg-10 col-lg-offset-1 text-right">				
				<a ng-click="salvar()"
					class="btn btn-small btn-primary btn-sm botoes-acoes"> Salvar
					<span class="glyphicon glyphicon-circle-arrow-right"></span>
				</a>
			</div>
		</div>
		<div>
			<div class="col-md-8 col-md-offset-1">				
				            <div class="control-group">
					<label class="control-label" for="codigo1">nome:</label>
					<div class="controls">
					<input type="text" id="nome" size="100" ng-model=".nome" value="<%=Pessoa.nome%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo2">identificacaofiscal:</label>
					<div class="controls">
					<input type="text" id="identificacaofiscal" size="100" ng-model=".identificacaofiscal" value="<%=Pessoa.identificacaofiscal%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo3">idsituacaocadastro:</label>
					<div class="controls">
					<input type="text" id="idsituacaocadastro" size="100" ng-model=".idsituacaocadastro"value="<%=Pessoa.idsituacaocadastro%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo4">idsituacaopessoa:</label>
					<div class="controls">
					<input type="text" id="idsituacaopessoa" size="100" ng-model=".idsituacaopessoa" value="<%=Pessoa.idsituacaopessoa%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo5">idsituacaoreceitafederal:</label>
					<div class="controls">
					<input type="text" id="idsituacaoreceitafederal" size="100" ng-model=".idsituacaoreceitafederal" value="<%=Pessoa.idsituacaoreceitafederal%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo6">datacadastro:</label>
					<div class="controls">
					<input type="text" id="datacadastro" size="100" ng-model=".datacadastro" value="<%=Pessoa.datacadastro%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo7">idagencia:</label>
					<div class="controls">
					<input type="text" id="idagencia" size="100" ng-model=".idagencia" value="<%=Pessoa.idagencia%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo8">idposto:</label>
					<div class="controls">
					<input type="text" id="idposto" size="100" ng-model=".idposto" value="<%=Pessoa.idposto%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo9">clientedesde:</label>
					<div class="controls">
					<input type="text" id="clientedesde" size="100" ng-model=".clientedesde" value="<%=Pessoa.clientedesde%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo10">idorigem:</label>
					<div class="controls">
					<input type="text" id="idorigem" size="100" ng-model=".idorigem" value="<%=Pessoa.idorigem	%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo11">status:</label>
					<div class="controls">
					<input type="text" id="status" size="100" ng-model=".status" value="<%=Pessoa.status%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo12">dataatualizacao:</label>
					<div class="controls">
					<input type="text" id="dataatualizacao" size="100" ng-model=".dataatualizacao" value="<%=Pessoa.dataatualizacao%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo13">idusuario:</label>
					<div class="controls">
					<input type="text" id="idusuario" size="100" ng-model=".idusuario" value="<%=Pessoa.idusuario%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo14">idestacao:</label>
					<div class="controls">
					<input type="text" id="idestacao" size="100" ng-model=".idestacao" value="<%=Pessoa.idestacao%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo15">dataatualizacaocadastro:</label>
					<div class="controls">
					<input type="text" id="dataatualizacaocadastro" size="100" ng-model=".dataatualizacaocadastro" value="<%=Pessoa.dataatualizacaocadastro%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo16">nomeusuario:</label>
					<div class="controls">
					<input type="text" id="nomeusuario" size="100" ng-model=".nomeusuario" value="<%=Pessoa.nomeusuario%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo17">email:</label>
					<div class="controls">
					<input type="text" id="email" size="100" ng-model=".email" value="<%=Pessoa.email%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo18">idnatrelacnegocio:</label>
					<div class="controls">
					<input type="text" id="idnatrelacnegocio" size="100" ng-model=".idnatrelacnegocio" value="<%=Pessoa.idnatrelacnegocio%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo19">nivelsigilo:</label>
					<div class="controls">
					<input type="text" id="nivelsigilo" size="100" ng-model=".nivelsigilo" value="<%=Pessoa.nivelsigilo%>"/>
					</div>
				</div>
            <div class="control-group">
					<label class="control-label" for="codigo20">idsubtiporegistro:</label>
					<div class="controls">
					<input type="text" id="idsubtiporegistro" size="100" ng-model=".idsubtiporegistro" value="<%=Pessoa.idsubtiporegistro%>"/>
					</div>
				</div>

			</div>
		</div>
		<barraacoestemplate />
		<div class="btn-group btn-group-justified">
			<div class="well titulo-tabela">
				<div class="verticalSpace">
					<div class="col-md-4">
						<label>Denomina&ccedil;&atilde;o: 
							<input type="text" ng-model="filtropessoa.idpessoa" ng-minlength="5" placeholder="Filtrar area" size="40"/>
						</label>
					</div>
					<div class="col-md-offset-5 col-md-3 text-right">
						<button ng-click="pesquisapessoa()" class="btn btn-small btn-primary btn-sm botoes-acoes ">Pesquisar</button>
					 </div>
				</div>
			</div>
		</div> 
	
		<div style="padding: 0px; margin-bottom: 5px;">
			<table class="table table-striped table-condensed">
				<thead>
					<tr class="login-header">
						<th style="min-width: 60px;">Denomina&ccedil;&atilde;o</th>
						<th style="width: 20px;">&nbsp;</th>
						<th style="width: 20px;">&nbsp;</th>
					</tr>
				</thead>
				<tbody>
					<% if(Pessoa.length > 0) { %>
						<% for(var i = 0; i < Pessoa.length; i++) {%>
                    <tr >
					    <td><%=pessoas.idpessoa %></td>
						<td><%= pessoas.nome %></td>
						<td><%= pessoas.identificacaofiscal %></td>
						<td><%= pessoas.idsituacaocadastro %></td>
						<td><%=pessoas.idsituacaopessoa %></td>
						<td><%=pessoas.idsituacaoreceitafederal %></td>
						<td><%=pessoas.datacadastro %></td>
						<td><%=pessoas.idagencia %></td>
						<td><%=pessoas.idposto %></td>
						<td><%=pessoas.clientedesde %></td>
						<td><%=pessoas.idorigem %></td>
						<td><%=pessoas.status %></td>
						<td><%=pessoas.dataatualizacao %></td>
						<td><%=pessoas.idusuario %></td>
						<td><%=pessoas.idestacao %></td>
						<td><%=pessoas.dataatualizacaocadastro %></td>
						<td><%=pessoas.nomeusuario %></td>
						<td><%=pessoas.email %></td>
						<td><%=pessoas.idnatrelacnegocio %></td>
						<td><%=pessoas.nivelsigilo %></td>
						<td><%=pessoas.idsubtiporegistro %></td>

						<td>
							<button class="btn btn-sm btn-warning" ng-click="editar(pessoa.idpessoa,pessoa.nome,pessoa.identificacaofiscal,pessoa.idsituacaocadastro,pessoa.idsituacaopessoa,pessoa.idsituacaoreceitafederal,pessoa.datacadastro,pessoa.idagencia,pessoa.idposto,pessoa.clientedesde,pessoa.idorigem,pessoa.status,pessoa.dataatualizacao,pessoa.idusuario,pessoa.idestacao,pessoa.dataatualizacaocadastro,pessoa.nomeusuario,pessoa.email,pessoa.idnatrelacnegocio,pessoa.nivelsigilo,pessoa.idsubtiporegistro,)">
								<i class="fa fa-edit"></i>
							</button>
						</td>
						<td>
							<button class="btn btn-sm btn-danger" ng-click="excluir(pessoa.identificador)">
								<i class="fa fa-trash-o"></i>
							</button>
						</td>
					</tr>
					<% } %>
			<% } %>
				</tbody>
	
			</table>
	
		</div>
		<pagination total-items="bigTotalItems" page="bigCurrentPage"
			style="margin: 0px 0;" max-size="maxSize" class="pagination-sm"
			boundary-links="true" rotate="false" num-pages="numPages"
			ng-click="setPage(bigCurrentPage)"></pagination>
	</div>
	</body>
</html>