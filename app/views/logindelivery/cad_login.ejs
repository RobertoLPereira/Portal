<!DOCTYPE html>
<html lang="pt-br">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
<head>
	<title>Cadastrar Usu√°rio</title>
	<link rel="stylesheet" type="text/css" href="css/style_menu.css">
	<link href="https://fonts.googleapis.com/css?family=Poppins:600&display=swap" rel="stylesheet">
	<script src="https://kit.fontawesome.com/a81368914c.js"></script>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<script src="https://www.mercadopago.com/v2/security.js" view="checkout" output="deviceId"></script>
</head>
<body  overflow: scroll; onload="Carregar(<%=JSON.stringify(Usuario)%>)">
	<img class="wave" src="img/wave.png">
	<div class="container">
		<div class="img">
			<img src="img/bg.svg">
		</div>
		<div class="login-content">
			<form action="/incpfdelivery" method="post" name="cadlogin" id="cadlogin">
				<img src="img/avatar.svg">
				<h1 class="title">Cadastre-se</h1>
				<div class="div">
						<% if(validacao.length > 0){ %>							
								<div class="alert alert-danger">
									<strong>Aten&ccedil;&atilde;o!</strong>
									<ul>
										<% for(var i = 0; i < validacao.length; i++) {%>
											<li><%=validacao[i].msg%></li>
										<% } %>
									</ul>
								</div>
							<% } %>
					</div>
	           		<div class="input-div one">
	           		   <div class="i">
	           		   		<i class="fas fa-user"></i>
	           		   </div>
	           		   
	           		   <div class="div">
	           		   		<h5>E-Mail ou Celular</h5>
	           		   		<input type="text" class="input" name="usuario" id="usuario">
	           		   </div>
	           		</div>
				   <div class="input-div one">
						<div class="i">
								<i class="fas fa-user"></i>
						</div>
						
						<div class="div">
								<h5>Nome completo</h5>
								<input type="text" class="input" name="nomeusuario" id="nomeusuario">
						</div>
				 	</div>
				   <div class="input-div one">
					<div class="i">
							<i class="fas fa-user"></i>
					</div>
					
					<div class="div">
							<h5>CPF/CNPJ</h5>
							<input type="text" class="input" name="identificacaofiscal" id="identificacaofiscal">
					</div>
					 </div>
	           		<div class="input-div pass">
	           		   <div class="i">
	           		    	<i class="fas fa-lock"></i>
	           		   </div>
	           		   <div class="div">
	           		    	<h5>Senha</h5>
	           		    	<input type="password" class="input" name="senha" id="senha">
	            	   </div>
	            	</div>
                <div class="input-div pass">
                    <div class="i">
                         <i class="fas fa-lock"></i>
                    </div>
                    <div class="div">
                         <h5>Repetir a senha</h5>
                         <input type="password" class="input" name="senha2" id="senha2">
                 </div>
              </div>
              <a href="/loginCompreaki">Login</a>
            	<input type="submit" class="btn" value="Confirmar">
            	<div class="div">
					<input type="hidden" class="input" name="idnatrelacnegocio" id="idnatrelacnegocio" value="1">
					<input type="hidden" class="input" name="deviceId" id="deviceId">
       		   </div>
            	<input type="hidden" name="paginar" id="paginar" value="Categorias">
            </form>
        </div>
    </div>
    <script type="text/javascript" src="js/main.js">
    	function Carregar(Usuario){
    		alert(Usuario);
    		var vForm = document.getElementById('cadlogin');
				vForm.usuario.value = Usuario.usuario;
				vForm.nomeusuario.value = Usuario.nomeusuario;
				vForm.identificacaofiscal.value = Usuario.identificacao;			
    	}
    </script>
</body>
</html>
