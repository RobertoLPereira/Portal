<accordion-group heading="listagem de possiveisachados" >

			<h4>Lista de Sistemas</h4>
			<table class="table table-striped table-condensed">
				<thead>
				    <tr class="login-header">
				        <th style="min-width: 60px;">Codigo</th>

				        <th style="min-width: 60px;">Nome Sistema</th>

				        <th style="min-width: 60px;">Sigla</th>

				        <th style="min-width: 60px;">Versao</th>

				        <th style="min-width: 60px;">Url</th>

                        <th style="width: 20px;">&nbsp;</th>
                        <th style="width: 20px;">&nbsp;</th>
                    </tr>
				</thead>
				<tbody>
					<% if(Sistemas.length > 0) { %>
	    				<% for(var i = 0; i < Sistemas.length; i++) {%>
							<tr>

								<td align="center"><%=Sistemas[i].idsistema%></td>
					            <td><%=Sistemas[i].nomesistema%></td>
					            <td><%=Sistemas[i].sigla%></td>
					            <td><%=Sistemas[i].versao%></td>
					            <td><%=Sistemas[i].url%></td>
					
								<td>
									<button class="btn" ng-click="editar(possiveisachados.identificador)">editar</button>

									<button class="btn" ng-click="excluir(possiveisachados.identificador)">excluir</button>

								</td>

							</tr>
						<% } %>
					<% } %>
				</tbody>

			</table>
	
	</accordion-group>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>